{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowPixelWaveAccountOnly",
            "Effect": "Deny",
            "Principal": "*",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::pixelwave-assets",
                "arn:aws:s3:::pixelwave-assets/*"
            ],
            "Condition": {
                "StringNotEquals": {
                    "aws:PrincipalAccount": "YOUR_ACCOUNT_ID"
                }
            }
        }
    ]
}